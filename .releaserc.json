{
  "branches": [
    {
      "name": "dev",
      "prerelease": true
    },
    {
      "name": "master"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "*.zip",
            "label": false
          },
          {
            "path": "LICENSE",
            "label": false
          }
        ]
      }
    ]
  ],
  "repositoryUrl": "https://github.com/PanoMC/sdk.git"
}